program alamat;
var
jalan,no,kota,kodepos : string;
begin
    write('Jalan    : '); readln(jalan);
    write('No       : '); readln(no);
    write('Kota     : '); readln(kota);
    write('Kode Pos : '); readln(kodepos);
    write('Alamat: Jalan ');
    write(jalan);
    write(' No. ');
    write(kota);
    write(' ');
    write(kodepos);

end.
